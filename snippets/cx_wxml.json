{
    "cx-data": {
        "prefix": "cx-data",
        "body": "{{$1}}",
        "description": "数据绑定"
    },
    "cx-if": {
        "prefix": "cx-if",
        "body": "wx:if=\"{{$1}}\"",
        "description": "条件渲染"
    },
    "cx-elif": {
        "prefix": "cx-elif",
        "body": "wx:elif=\"{{$1}}\"",
        "description": ""
