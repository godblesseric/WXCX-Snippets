    "cx-else": {
        "prefix": "cx-else",
        "body": "wx:else",
        "description": ""
    },
    "cx-if-block": {
        "prefix": "cx-if-block",
        "body": [
            "<block wx:if=\"{{$1}}\">",
            "$2",
            "</block>"
        ],
        "description": ""
    },
    "cx-elif-block": {
        "prefix": "cx-elif-block",
        "body": [
            "<block wx:elif=\"{{$1}}\">",
            "$2",
            "</block>"
        ],
        "description": ""
    },
    "cx-else-block": {
        "prefix": "cx-else-block",
        "body": [
            "<block wx:else>",
            "$1",
            "</block>"
        ],
        "description": ""
    },
    "cx-for": {
        "prefix": "cx-for",
        "body": "wx:for=\"{{$1}}\"",
        "description": "列表渲染"
    },
    "cx-for-index": {
        "prefix": "cx-for-index",
        "body": "wx:for-index=\"$1\"",
        "description": ""
    },
    "cx-for-item": {
        "prefix": "cx-for-item",
        "body": "wx:for-item=\"$1\"",
        "description": ""
    },
    "cx-for-block": {
        "prefix": "cx-for-block",
        "body": [
            "<block wx:for=\"{{$1}}\">",
            "$2",
            "</block>"
        ],
        "description": ""
    },
    "cx-key": {
        "prefix": "cx-key",
        "body": "wx:key=\"$unique\"",
        "description": ""
    },
    "cx-template": {
        "prefix": "cx-template",
        "body": [
            "<template name=\"$name\">",
            "$1",
            "</template>"
        ],
        "description": "模板"
    },
    "cx-template-is": {
        "prefix": "cx-template-is",
        "body": "<template is=\"$1\" data=\"{{$2}}\"/>",
        "description": ""
    },
    "cx-import": {
        "prefix": "cx-import",
        "body": "<import src=\"$1\"/>",
        "description": "import可以在该文件中使用目标文件定义的template"
    },
    "cx-include": {
        "prefix": "cx-include",
        "body": "<include src=\"$1\"/>",
        "description": "include可以将目标文件除了<template/>的整个代码引入，相当于是拷贝到include位置"
    },
    "cx-bind-touchstart": {
        "prefix": "cx-bind-touchstart",
        "body": "bindtouchstart=\"$1\"",
        "description": "手指触摸动作开始"
    },
    "cx-bind-touchmove": {
        "prefix": "cx-bind-touchmove",
        "body": "bindtouchmove=\"$1\"",
        "description": "手指触摸后移动"
    },
    "cx-bind-touchcancel": {
        "prefix": "cx-bind-touchcancel",
        "body": "bindtouchcancel=\"$1\"",
        "description": "手指触摸动作被打断，如来电提醒，弹窗"
    },
    "cx-bind-touchend": {
        "prefix": "cx-bind-touchend",
        "body": "bindtouchend=\"$1\"",
        "description": "手指触摸动作结束"
    },
    "cx-bind-tap": {
        "prefix": "cx-bind-tap",
        "body": "bindtap=\"$1\"",
        "description": "手指触摸后马上离开"
    },
    "cx-bind-longtap": {
        "prefix": "cx-bind-longtap",
        "body": "bindlongtap=\"$1\"",
        "description": "手指触摸后，超过350ms再离开"
    },
    "cx-bind-data": {
        "prefix": "cx-bind-data",
        "body": "data-$1=\"$2\"",
        "description": ""
    },
    "cx-catch-touchstart": {
        "prefix": "cx-catch-touchstart",
        "body": "catchtouchstart=\"$1\"",
        "description": "手指触摸动作开始"
    },
    "cx-catch-touchmove": {
        "prefix": "cx-catch-touchmove",
        "body": "catchtouchmove=\"$1\"",
        "description": "手指触摸后移动"
    },
    "cx-catch-touchcancel": {
        "prefix": "cx-catch-touchcancel",
        "body": "catchtouchcancel=\"$1\"",
        "description": "手指触摸动作被打断，如来电提醒，弹窗"
    },
    "cx-catch-touchend": {
        "prefix": "cx-catch-touchend",
        "body": "catchtouchend=\"$1\"",
        "description": "手指触摸动作结束"
    },
    "cx-catch-tap": {
        "prefix": "cx-catch-tap",
        "body": "catchtap=\"$1\"",
        "description": "手指触摸后马上离开"
    },
    "cx-catch-longtap": {
        "prefix": "cx-catch-longtap",
        "body": "catchlongtap=\"$1\"",
        "description": "手指触摸后，超过350ms再离开"
    },
    "cx-view": {
        "prefix": "cx-view",
        "body": [
            "<view>",
            "$1",
            "</view>"
        ],
        "description": "视图容器。"
    },
    "cx-view-hover": {
        "prefix": "cx-view-hover",
        "body": "hover=\"true\" hover-class=\"$1\"",
        "description": ""
    },
    "cx-scroll-view": {
        "prefix": "cx-scroll-view",
        "body": [
            "<scroll-view>",
            "$1",
            "</scroll-view>"
        ],
        "description": "可滚动视图区域。"
    },
    "cx-scroll-view-scroll-x": {
        "prefix": "cx-scroll-x",
        "body": " scroll-x=\"true\"",
        "description": "允许横向滚动"
    },
    "cx-scroll-view-scroll-y": {
        "prefix": "cx-scroll-y",
        "body": "scroll-y=\"true\"",
        "description": "允许纵向滚动"
    },
    "cx-scroll-view-upper-threshold": {
        "prefix": "cx-scroll-upper-threshold",
        "body": "upper-threshold=\"50\"",
        "description": "距顶部/左边多远时（单位px），触发 scrolltoupper 事件"
    },
    "cx-scroll-view-lower-threshold": {
        "prefix": "cx-scroll-lower-threshold",
        "body": "lower-threshold=\"50\"",
        "description": "距底部/右边多远时（单位px），触发 scrolltolower 事件"
    },
    "cx-scroll-view-scroll-top": {
        "prefix": "cx-scroll-top",
        "body": "scroll-top=\"$1\"",
        "description": "设置竖向滚动条位置"
    },
    "cx-scroll-view-scroll-left": {
        "prefix": "cx-scroll-left",
        "body": "scroll-left=\"$1\"",
        "description": "设置横向滚动条位置"
    },
    "cx-scroll-view-scroll-into-view": {
        "prefix": "cx-scroll-into-view",
        "body": "scroll-into-view=\"$1\"",
        "description": "值应为某子元素id，则滚动到该元素，元素顶部对齐滚动区域顶部"
    },
    "cx-scroll-view-bindscrolltoupper": {
        "prefix": "cx-scroll-bindscrolltoupper",
        "body": "bindscrolltoupper=\"$1\"",
        "description": "滚动到顶部/左边，会触发 scrolltoupper 事件"
    },
    "cx-scroll-view-bindscrolltolower": {
        "prefix": "cx-scroll-bindscrolltolower",
        "body": "bindscrolltolower=\"$1\"",
        "description": "滚动到底部/右边，会触发 scrolltolower 事件"
    },
    "cx-scroll-view-bindscroll": {
        "prefix": "cx-scroll-bindscroll",
        "body": "bindscroll=\"$1\"",
        "description": "滚动时触发，event.detail = {scrollLeft, scrollTop, scrollHeight, scrollWidth, deltaX, deltaY}"
    },
    "cx-swiper": {
        "prefix": "cx-swiper",
        "body": [
            "<swiper>",
            "$1",
            "</swiper>"
        ],
        "description": "滑块视图容器。"
    },
    "cx-swiper-item": {
        "prefix": "cx-swiper-item",
        "body": [
            "<swiper-item>",
            "$1",
            "</swiper-item>"
        ],
        "description": ""
    },
    "cx-swiper-indicator-dots": {
        "prefix": "cx-swiper-indicator-dots",
        "body": "indicator-dots=\"true\"",
        "description": "是否显示面板指示点"
    },
    "cx-swiper-indicator-color": {
        "prefix": "cx-swiper-indicator-color",
        "body": "indicator-color=\"rgba(0, 0, 0, .3)\"",
        "description": "指示点颜色 （这个属性目前暂未启用）"
    },
    "cx-swiper-indicator-active-color": {
        "prefix": "cx-swiper-indicator-active-color",
        "body": "indicator-active-color=\":#000000\"",
        "description": "当前选中的指示点颜色 （这个属性目前暂未启用）"
    },
    "cx-swiper-autoplay": {
        "prefix": "cx-swiper-autoplay",
        "body": "autoplay=\"true\"",
        "description": "是否自动切换"
    },
    "cx-swiper-current": {
        "prefix": "cx-swiper-current",
        "body": "current=\"0\"",
        "description": "当前所在页面的 index"
    },
    "cx-swiper-interval": {
        "prefix": "cx-swiper-interval",
        "body": "interval=\"5000\"",
        "description": "自动切换时间间隔"
    },
    "cx-swiper-duration": {
        "prefix": "cx-swiper-duration",
        "body": "duration=\"500\"",
        "description": "滑动动画时长"
    },
    "cx-swiper-circular": {
        "prefix": "cx-swiper-circular",
        "body": "circular=\"true\"",
        "description": "是否采用衔接滑动"
    },
    "cx-swiper-bindchange": {
        "prefix": "cx-swiper-bindchange",
        "body": "bindchange=\"$1\"",
        "description": "current 改变时会触发 change 事件，event.detail = {current: current}"
    },
    "cx-icon": {
        "prefix": "cx-icon",
        "body": "<icon type=\"$1\"/>",
        "description": "图标。"
    },
    "cx-icon-success": {
        "prefix": "cx-icon-success",
        "body": "success",
        "description": ""
    },
    "cx-icon-success_no_circle": {
        "prefix": "cx-icon-success_no_circle",
        "body": "success_no_circle",
        "description": ""
    },
    "cx-icon-info": {
        "prefix": "cx-icon-info",
        "body": "info",
        "description": ""
    },
    "cx-icon-warn": {
        "prefix": "cx-icon-warn",
        "body": "warn",
        "description": ""
    },
    "cx-icon-waiting": {
        "prefix": "cx-icon-waiting",
        "body": "waiting",
        "description": ""
    },
    "cx-icon-cancel": {
        "prefix": "cx-icon-cancel",
        "body": "cancel",
        "description": ""
    },
    "cx-icon-download": {
        "prefix": "cx-icon-download",
        "body": "download",
        "description": ""
    },
    "cx-icon-search": {
        "prefix": "cx-icon-search",
        "body": "search",
        "description": ""
    },
    "cx-icon-clear": {
        "prefix": "cx-icon-clear",
        "body": "clear",
        "description": ""
    },
    "cx-icon-size": {
        "prefix": "cx-icon-size",
        "body": "size=\"23\"",
        "description": "icon的大小，单位px"
    },
    "cx-icon-color": {
        "prefix": "cx-icon-color",
        "body": "color=\"$color\"",
        "description": "icon的颜色，同css的color"
    },
    "cx-text": {
        "prefix": "cx-text",
        "body": "<text>$1</text>",
        "description": "文本。"
    },
    "cx-progress": {
        "prefix": "cx-progress",
        "body": "<progress percent=\"$1\" />",
        "description": "进度条。"
    },
    "cx-progress-show-info": {
        "prefix": "cx-progress-show-info",
        "body": "show-info=\"true\"",
        "description": "在进度条右侧显示百分比"
    },
    "cx-progress-stroke-width": {
        "prefix": "cx-progress-stroke-width",
        "body": "stroke-width=\"6\"",
        "description": "进度条线的宽度，单位px"
    },
    "cx-progress-color": {
        "prefix": "cx-progress-color",
        "body": "color=\"#09BB07\"",
        "description": "进度条颜色"
    },
    "cx-progress-active": {
        "prefix": "cx-progress-active",
        "body": "active=\"true\"",
        "description": "进度条从左往右的动画"
    },
    "cx-button": {
        "prefix": "cx-button",
        "body": "<button>$1</button>",
        "description": "按钮。"
    },
    "cx-button-size": {
        "prefix": "cx-button-size",
        "body": "size=\"$default\"",
        "description": "有效值 default, mini"
    },
    "cx-button-type": {
        "prefix": "cx-button-type",
        "body": "type=\"$default\"",
        "description": "按钮的样式类型，有效值 primary, default, warn"
    },
    "cx-button-plain": {
        "prefix": "cx-button-plain",
        "body": "plain=\"true\"",
        "description": "按钮是否镂空，背景色透明"
    },
    "cx-button-disabled": {
        "prefix": "cx-button-disabled",
        "body": "disabled=\"true\"",
        "description": "是否禁用"
    },
    "cx-button-loading": {
        "prefix": "cx-button-loading",
        "body": "loading=\"true\"",
        "description": "名称前是否带 loading 图标"
    },
    "cx-button-form-type": {
        "prefix": "cx-button-form-type",
        "body": "form-type=\"$submit\"",
        "description": "有效值：submit, reset，用于 <form/> 组件，点击分别会触发 submit/reset 事件"
    },
    "cx-button-hover-class": {
        "prefix": "cx-button-hover-class",
        "body": "hover-class=\"$1\"",
        "description": "指定按钮按下去的样式类。当 hover-class=\"none\" 时，没有点击态效果"
    },
    "cx-button-hover-start-time": {
        "prefix": "cx-button-hover-start-time",
        "body": "hover-start-time=\"50\"",
        "description": "按住后多久出现点击态，单位毫秒"
    },
    "cx-button-hover-stay-time": {
        "prefix": "cx-button-hover-stay-time",
        "body": "hover-stay-time=\"400\"",
        "description": "手指松开后点击态保留时间，单位毫秒"
    },
    "cx-checkbox-group": {
        "prefix": "cx-checkbox-group",
        "body": [
            "<checkbox-group name=\"$name\" bindchange=\"$2\">",
            "$2",
            "</checkbox-group>"
        ],
        "description": "<checkbox-group/>中选中项发生改变是触发 change 事件，detail = {value:[选中的checkbox的value的数组]}"
    },
    "cx-checkbox": {
        "prefix": "cx-checkbox",
        "body": "<label><checkbox value=\"$1\" />$2</label>",
        "description": "多选项目"
    },
    "cx-checkbox-disabled": {
        "prefix": "cx-checkbox-disabled",
        "body": "disabled=\"true\"",
        "description": "是否禁用"
    },
    "cx-checkbox-checked": {
        "prefix": "cx-checkbox-checked",
        "body": "checked=\"true\"",
        "description": "当前是否选中，可用来设置默认选中"
    },
    "cx-checkbox-color": {
        "prefix": "cx-checkbox-color",
        "body": "color=\"$color\"",
        "description": "checkbox的颜色，同css的color"
    },
    "cx-form": {
        "prefix": "cx-form",
        "body": [
            "<form bindsubmit=\"$1\">",
            "<button formType=\"submit\">$2</button>",
            "</form>"
        ],
        "description": "表单，将组件内的用户输入的<switch/> <input/> <checkbox/> <slider/> <radio/> <picker/> 提交。"
    },
    "cx-form-report-submit": {
        "prefix": "cx-form-report-submit",
        "body": "report-submit=\"true\"",
        "description": "是否返回 formId 用于发送模板消息"
    },
    "cx-form-bindsubmit": {
        "prefix": "cx-form-bindsubmit",
        "body": "bindsubmit=\"$1\"",
        "description": "携带 form 中的数据触发 submit 事件，event.detail = {value : {'name': 'value'} , formId: ''}"
    },
    "cx-form-bindreset": {
        "prefix": "cx-form-bindreset",
        "body": "bindreset=\"$1\"",
        "description": "表单重置时会触发 reset 事件"
    },
    "cx-input": {
        "prefix": "cx-input",
        "body": "<input name=\"$name\" />",
        "description": "输入框。"
    },
    "cx-input-value": {
        "prefix": "cx-input-value",
        "body": "value=\"$value\"",
        "description": "输入框的初始内容"
    },
    "cx-input-type": {
        "prefix": "cx-input-type",
        "body": "type=\"$text\"",
        "description": "input 的类型，有效值：text, number, idcard, digit"
    },
    "cx-input-password": {
        "prefix": "cx-input-password",
        "body": "password=\"true\"",
        "description": "是否是密码类型"
    },
    "cx-input-placeholder": {
        "prefix": "cx-input-placeholder",
        "body": "placeholder=\"$placeholder\"",
        "description": "输入框为空时占位符"
    },
    "cx-input-placeholder-style": {
        "prefix": "cx-input-placeholder-style",
        "body": "placeholder-style=\"$1\"",
        "description": "指定 placeholder 的样式"
    },
    "cx-input-placeholder-class": {
        "prefix": "cx-input-placeholder-class",
        "body": "placeholder-class=\"$1\"",
        "description": "指定 placeholder 的样式类"
    },
    "cx-input-disabled": {
        "prefix": "cx-input-disabled",
        "body": "disabled=\"true\"",
        "description": "是否禁用"
    },
    "cx-input-maxlength": {
        "prefix": "cx-input-maxlength",
        "body": "maxlength=\"-1\"",
        "description": "最大输入长度，设置为 -1 的时候不限制最大长度"
    },
    "cx-input-cursor-spacing": {
        "prefix": "cx-input-cursor-spacing",
        "body": "cursor-spacing=\"0\"",
        "description": "指定光标与键盘的距离，单位 px 。取 input 距离底部的距离和 cursor-spacing 指定的距离的最小值作为光标与键盘的距离"
    },
    "cx-input-focus": {
        "prefix": "cx-input-focus",
        "body": "focus=\"true\"",
        "description": "获取焦点"
    },
    "cx-input-bindinput": {
        "prefix": "cx-input-bindinput",
        "body": "bindinput=\"$1\"",
        "description": "当键盘输入时，触发input事件，event.detail = {value: value}，处理函数可以直接 return 一个字符串，将替换输入框的内容。"
    },
    "cx-input-bindfocus": {
        "prefix": "cx-input-bindfocus",
        "body": "bindfocus=\"$1\"",
        "description": "输入框聚焦时触发，event.detail = {value: value}"
    },
    "cx-input-bindblur": {
        "prefix": "cx-input-bindblur",
        "body": "bindblur=\"$1\"",
        "description": "输入框失去焦点时触发，event.detail = {value: value}"
    },
    "cx-input-bindconfirm": {
        "prefix": "cx-input-bindconfirm",
        "body": "bindconfirm=\"$1\"",
        "description": "点击完成按钮时触发，event.detail = {value: value}"
    },
    "cx-label": {
        "prefix": "cx-label",
        "body": [
            "<label>",
            "$1",
            "</label>"
        ],
        "description": "目前可以绑定的控件有：<button/>, <checkbox/>, <radio/>, <switch/>。"
    },
    "cx-picker-selector": {
        "prefix": "cx-picker-selector",
        "body": [
            "<picker name=\"name\" mode=\"selector\" range=\"$range\" range-key=\"$key\">",
            "<view>",
            "$1",
            "</view>",
            "</picker>"
        ],
        "description": "普通选择器"
    },
    "cx-picker-time": {
        "prefix": "cx-picker-time",
        "body": [
            "<picker name=\"name\" mode=\"time\" start=\"$start\" end=\"$end\">",
            "<view>",
            "$1",
            "</view>",
            "</picker>"
        ],
        "description": "时间选择器，格式为\"hh:mm\""
    },
    "cx-picker-date": {
        "prefix": "cx-picker-date",
        "body": [
            "<picker name=\"name\" mode=\"date\" start=\"$start\" end=\"$end\" fields=\"$day\">",
            "<view>",
            "$1",
            "</view>",
            "</picker>"
        ],
        "description": "日期选择器，格式为\"YYYY-MM-DD\",fields有效值 year,month,day，表示选择器的粒度"
    },
    "cx-picker-bindchange": {
        "prefix": "cx-picker-bindchange",
        "body": "bindchange=\"$1\"",
        "description": "value 改变时触发 change 事件，event.detail = {value: value}"
    },
    "cx-picker-disabled": {
        "prefix": "cx-picker-disabled",
        "body": "disabled=\"true\"",
        "description": "是否禁用"
    },
    "cx-picker-view": {
        "prefix": "cx-picker-view",
        "body": [
            "<picker-view bindchange=\"$1\">",
            "$2",
            "</picker-view>"
        ],
        "description": "嵌入页面的滚动选择器"
    },
    "cx-picker-view-column": {
        "prefix": "cx-picker-view-column",
        "body": [
            "<picker-view-column>",
            "<view>",
            "$1",
            "</view>",
            "</picker-view-column>"
        ],
        "description": ""
    },
    "cx-picker-view-value": {
        "prefix": "cx-picker-view-value",
        "body": "value=\"$1\"",
        "description": "数组中的数字依次表示 picker-view 内的 picker-view-colume 选择的第几项（下标从 0 开始），数字大于 picker-view-column 可选项长度时，选择最后一项。"
    },
    "cx-picker-view-indicator-style": {
        "prefix": "cx-picker-view-indicator-style",
        "body": "indicator-style=\"$1\"",
        "description": "设置选择器中间选中框的样式"
    },
    "cx-picker-view-bindchange": {
        "prefix": "cx-picker-view-bindchange",
        "body": "bindchange=\"$1\"",
