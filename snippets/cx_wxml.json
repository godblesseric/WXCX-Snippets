        "prefix": "wx-scroll-view",
        "body": [
            "<scroll-view class=\"$1\">",
            "$2",
            "</scroll-view>"
        ],
        "description": "可滚动视图区域。"
    },
    "wx--scroll-view-scroll-x": {
        "prefix": "wx-scroll-x",
        "body": " scroll-x=\"true\"",
        "description": "允许横向滚动"
    },
